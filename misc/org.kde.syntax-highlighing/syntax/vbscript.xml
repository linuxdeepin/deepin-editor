		<contexts>
			<context attribute="Normal Text" lineEndContext="#pop" name="Normal Text">
				<keyword attribute="Keywords" context="#stay" String="vbsKeywords" />
				<keyword attribute="Functions" context="#stay" String="internalFunction" />
				<keyword attribute="Const" context="#stay" String="internalConst" />
				<!-- String text with "" -->
				<DetectChar attribute="String" context="string" char="&quot;" />
				<!-- Comment line with ' -->
				<DetectChar char="&apos;" context="Comment" firstNonSpace="false" />
				<Float attribute="Float" />
				<Int attribute="Int" />
				<!-- Detect variables at last -->
				<RegExpr attribute="Variables" context="#stay" String="\b[_a-zA-Z]\w*\b(?=( *=))"/>
			</context>
			<context attribute="String" lineEndContext="#stay" name="string">
				<DetectChar attribute="String" context="#pop" char="&quot;" />
			</context>
			<context attribute="Comment" lineEndContext="#pop" name="Comment">
			</context>
		</contexts>
